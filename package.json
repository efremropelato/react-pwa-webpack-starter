{
  "name": "es6-webpack-starter",
  "version": "0.2.0",
  "description": "A starter project to work with vanilla ES6 using Babel and Webpack",
  "repository": {
    "type": "git",
    "url": "git@github.com:oPauloChaves/es6-webpack2-starter.git"
  },
  "author": "Paulo Chaves <opaulochaves@mail.com>",
  "license": "MIT",
  "engines": {
    "node": ">=7.6"
  },
  "scripts": {
    "clean": "rimraf dist",
    "test": "echo \"Error: no test specified\" && exit 1",
    "start": "webpack-dev-server --open --config ./config/webpack.dev.js",
    "build": "npm run clean && webpack --config ./config/webpack.prod.js",
    "format": "prettier --write \"{src,config}/**/*.js\"",
    "lint": "eslint src config",
    "lint:fix": "npm run lint -- --fix"
  },
  "devDependencies": {
    "autoprefixer": "^7.1.2",
    "babel-core": "^7.0.0-beta.1",
    "babel-loader": "^7.1.1",
    "babel-preset-env": "^2.0.0-beta.1",
    "babel-preset-react": "^7.0.0-beta.2",
    "compression-webpack-plugin": "^0.4.0",
    "copy-webpack-plugin": "^4.0.1",
    "css-loader": "^0.28.0",
    "eslint": "^4.6.1",
    "eslint-config-standard": "^10.2.1",
    "eslint-config-standard-react": "^5.0.0",
    "eslint-plugin-import": "^2.7.0",
    "eslint-plugin-node": "^5.1.1",
    "eslint-plugin-promise": "^3.5.0",
    "eslint-plugin-react": "^7.4.0",
    "eslint-plugin-standard": "^3.0.1",
    "extract-text-webpack-plugin": "^3.0.0",
    "file-loader": "^0.11.2",
    "html-webpack-plugin": "^2.29.0",
    "node-sass": "^4.5.3",
    "postcss-loader": "^2.0.6",
    "prettier": "^1.8.2",
    "rimraf": "^2.6.1",
    "sass-loader": "^6.0.6",
    "style-loader": "^0.18.2",
    "url-loader": "^0.5.8",
    "webpack": "^3.6.0",
    "webpack-dev-server": "^2.9.1",
    "workbox-webpack-plugin": "2.0.2-rc1"
  },
  "dependencies": {
    "normalize.css": "^7.0.0",
    "react": "^16.1.1",
    "react-dom": "^16.1.1"
  },
  "prettier": {
    "singleQuote": false,
    "printWidth": 80,
    "semi": false,
    "trailingComma": "all",
    "bracketSpacing": false,
    "jsxBracketSameLine": false
  }
}
